# .gitignore - a merge of Flutter create and GPT5

# Miscellaneous
*.class
*.lock
*.log
*.pyc
*.swp
.DS_Store
.atom/
.build/
.buildlog/
.history
.svn/
.swiftpm/
migrate_working_dir/

# IntelliJ related
*.iml
*.ipr
*.iws
.idea/

# The .vscode folder contains launch configuration and tasks you configure in
# VS Code which you may wish to be included in version control, so this line
# is commented out by default.
.vscode/
*.code-workspace

# Flutter/Dart/Pub related
**/doc/api/
**/ios/Flutter/.last_build_id
.dart_tool/
.flutter-plugins
.flutter-plugins-dependencies
.packages
.pub-cache/
.pub/

# Build outputs
build/
ios/Pods/
ios/.symlinks/
ios/Flutter/Flutter.framework/
ios/Flutter/Flutter.podspec

# Symbolication related
app.*.symbols

# Obfuscation related
app.*.map.json

# Android Studio will place build artifacts here
/android/app/debug
/android/app/profile
/android/app/release

# Android related
.gradle/
local.properties
*.keystore
*.jks

# Web related
lib/generated_plugin_registrant.dart

# Windows related
windows/ephemeral/

# macOS related
macos/Flutter/ephemeral/

# Linux related
linux/flutter/ephemeral/

# Coverage
coverage/

# Exceptions to keep specific files
!/packages/flutter_tools/test/data/dart_dependencies_test/**/.packages
